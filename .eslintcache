:\\Users\\User\\Desktop\\react-pizza\\src\\pages\\index.js",[],"C:\\Users\\User\\Desktop\\react-pizza\\src\\components\\index.js",[],"C:\\Users\\User\\Desktop\\react-pizza\\src\\redux\\reducers\\index.js",[],"C:\\Users\\User\\Desktop\\react-pizza\\src\\redux\\reducers\\filters.js",[],"C:\\Users\\User\\Desktop\\react-pizza\\src\\redux\\reducers\\cart.js",[],"C:\\Users\\User\\Desktop\\react-pizza\\src\\redux\\reducers\\pizzas.js",[],"C:\\Users\\User\\Desktop\\react-pizza\\src\\pages\\Home.jsx",["87","88","89"],"import React from \"react\";\nimport { Categories, SortPizza, PizzaBlock, LoadingBlock } from \"../components\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport { setCategory, setSortBy } from \"../redux/actions/filter\";\nimport { fetchPizzas } from \"../redux/actions/pizzas\";\n\nconst categoriesNames = [\n  \"Мясные\",\n  \"Вегетарианская\",\n  \"Гриль\",\n  \"Острые\",\n  \"Закрытые\",\n];\nconst sortItems = [\n  { name: \"популярности\", type: \"popular\", order: \"desc\" },\n  { name: \"цене\", type: \"price\", order: \"desc\" },\n  { name: \"алфавиту\", type: \"name\", order: \"asc\" },\n];\n\nfunction Home() {\n  const dispatch = useDispatch();\n  const items = useSelector(({ pizzas }) => pizzas.items);\n  const cartItems = useSelector(({ cart }) => cart.items);\n  const isLoaded = useSelector(({ pizzas }) => pizzas.isLoaded);\n  const { sortBy, category } = useSelector(({ filters }) => filters);\n\n  const onSelectCategory = React.useCallback((index) => {\n    dispatch(setCategory(index));\n  }, []);\n  const onSelectSortBy = React.useCallback((type) => {\n    dispatch(setSortBy(type));\n  }, []);\n\n  const handleAddPizzaToCart = (obj) => {\n    dispatch({\n      type: 'ADD_PIZZA_CART',\n      payload: obj,\n    });\n  };\n\n  React.useEffect(() => {\n    dispatch(fetchPizzas(sortBy, category));\n  }, [sortBy, category]);\n\n\n\n  return (\n    <div className=\"container\">\n      <div className=\"content__top\">\n        <Categories\n          activCategory={category}\n          onClickCategory={onSelectCategory}\n          items={categoriesNames}\n        />\n\n        <SortPizza\n          activSortType={sortBy.type}\n          items={sortItems}\n          onClickSort={onSelectSortBy}\n        />\n      </div>\n      <h2 className=\"content__title\">Все пиццы</h2>\n      <div className=\"content__items\">\n        {isLoaded\n          ? items.map((obj) => (\n            <PizzaBlock\n              onClickAddPizza={handleAddPizzaToCart}\n              key={obj.id}\n              addedCount={cartItems[obj.id] && cartItems[obj.id].items.length}\n              {...obj}\n            />\n          ))\n          : Array(12)\n            .fill(0)\n            .map((_, index) => <LoadingBlock key={index} />)}\n      </div>\n    </div>\n  );\n}\n\nexport default Home;\n","C:\\Users\\User\\Desktop\\react-pizza\\src\\components\\sortPizza.jsx",[],"C:\\Users\\User\\Desktop\\react-pizza\\src\\components\\categories.jsx",[],"C:\\Users\\User\\Desktop\\react-pizza\\src\\components\\button.jsx",[],"C:\\Users\\User\\Desktop\\react-pizza\\src\\pages\\Cart.jsx",[],"C:\\Users\\User\\Desktop\\react-pizza\\src\\components\\header.jsx",[],"C:\\Users\\User\\Desktop\\react-pizza\\src\\components\\cartItem.jsx",[],"C:\\Users\\User\\Desktop\\react-pizza\\src\\components\\PizzaBlock\\index.jsx",[],"C:\\Users\\User\\Desktop\\react-pizza\\src\\components\\PizzaBlock\\LoadingBlock.jsx",[],"C:\\Users\\User\\Desktop\\react-pizza\\src\\redux\\actions\\pizzas.js",[],"C:\\Users\\User\\Desktop\\react-pizza\\src\\redux\\actions\\filter.js",[],"C:\\Users\\User\\Desktop\\react-pizza\\src\\redux\\actions\\cart.js",[],{"ruleId":"90","severity":1,"message":"91","line":29,"column":6,"nodeType":"92","endLine":29,"endColumn":8,"suggestions":"93"},{"ruleId":"90","severity":1,"message":"91","line":32,"column":6,"nodeType":"92","endLine":32,"endColumn":8,"suggestions":"94"},{"ruleId":"90","severity":1,"message":"95","line":43,"column":6,"nod